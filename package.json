{
  "name": "react-admin-template",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "bootstrap": "yarn || npm i",
    "clean": "rimraf dist",
    "start": "cross-env PORT=8888 umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "build": "umi build",
    "build:dev": "cross-env SERVER_ENV=dev umi build",
    "build:test": "cross-env SERVER_ENV=test umi build",
    "build:prod": "cross-env SERVER_ENV=production umi build",
    "build:web": "yarn build && cd ./dist && anywhere -h localhost -p 8080",
    "functions:build": "netlify-lambda build ./lambda",
    "functions:run": "cross-env NODE_ENV=dev netlify-lambda serve ./lambda",
    "site": "npm run build && npm run functions:build",
    "gh-pages": "yarn build:prod && gh-pages -d dist",
    "lint:ts": "tslint --project tsconfig.json --format codeFrame",
    "lint:style": "stylelint --fix 'src/**/*.less' --syntax less",
    "docker:build": "./scripts/docker-build.sh",
    "tsc": "tsc",
    "test": "umi test",
    "test:component": "umi test ./src/components",
    "deploy": "yarn build && rm -rf node_modules/gh-pages/.cache && gh-pages -d dist"
  },
  "repository": "https://github.com/ts-react/react-admin-template.git",
  "author": "JiuMao FE Team",
  "license": "MIT",
  "dependencies": {
    "@alitajs/antd-plus": "^1.0.6",
    "@alitajs/autils": "^0.2.0",
    "@jiumao/rc-table": "^0.0.2",
    "antd": "^3.21.2",
    "axios": "^0.19.0",
    "braft-editor": "^2.2.9",
    "classnames": "^2.2.6",
    "dva": "^2.6.0-beta.12",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.10",
    "lodash-decorators": "^6.0.0",
    "memoize-one": "^5.0.0",
    "moment": "^2.22.2",
    "nprogress": "^0.2.0",
    "numeral": "^2.0.6",
    "path-to-regexp": "^3.0.0",
    "qs": "^6.7.0",
    "rc-animate": "^2.4.4",
    "rc-charts": "^0.2.7",
    "react": "^16.8.6",
    "react-container-query": "^0.11.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.8.6",
    "react-image": "^2.1.3",
    "react-media-hook2": "^1.0.5",
    "react-text-mask": "^5.4.3",
    "react-transition-group": "^2.4.0",
    "react-virtualized": "^9.21.0",
    "rollbar": "^2.4.4",
    "screenfull": "^4.2.0",
    "store": "^2.0.12"
  },
  "devDependencies": {
    "@alitajs/umi-plugin-css-modules": "^0.0.1",
    "@alitajs/umi-plugin-lodash": "^0.1.0",
    "@babel/runtime": "^7.4.2",
    "@commitlint/cli": "^7.1.1",
    "@jiumao/commitlint-config": "^0.1.3",
    "@types/classnames": "^2.2.6",
    "@types/draft-js": "^0.10.26",
    "@types/jest": "^23.3.9",
    "@types/js-cookie": "^2.2.0",
    "@types/lodash": "^4.14.116",
    "@types/memoize-one": "^3.1.1",
    "@types/node": "^10.5.7",
    "@types/nprogress": "^0.0.29",
    "@types/qs": "^6.5.1",
    "@types/react": "^16.8.23",
    "@types/react-document-title": "^2.0.2",
    "@types/react-dom": "^16.8.4",
    "@types/react-text-mask": "^5.4.2",
    "@types/react-virtualized": "^9.21.1",
    "@types/store": "^2.0.0",
    "babel-plugin-import": "^1.12.1",
    "body-parser": "^1.19.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.14.1",
    "express": "^4.17.1",
    "gh-pages": "^2.0.1",
    "husky": "^2.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.7",
    "lotus-lint": "^0.0.1",
    "mockjs": "^1.0.1-beta3",
    "netlify-lambda": "^1.5.1",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.2",
    "serverless-http": "^2.0.2",
    "stylelint": "^10.0.1",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "stylelint-order": "^3.0.0",
    "stylelint-prettier": "^1.1.1",
    "typescript": "^3.5.3",
    "umi": "^2.9.3",
    "umi-plugin-react": "^1.9.19",
    "umi-types": "^0.3.17"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,ts,tsx,md,json,jsx,less}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.9.0"
  }
}
