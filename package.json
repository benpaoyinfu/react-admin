{
  "name": "react-admin",
  "version": "2.0.0",
  "description": "react development template",
  "main": "index.js",
  "repository": "git@github.com:pansyjs/react-admin.git",
  "author": "kang <wang_xingkang@qq.com>",
  "license": "MIT",
  "scripts": {
    "analyze": "cross-env ANALYZE=1 umi build",
    "start": "npm run start:dev",
    "build": "umi build",
    "test": "walrus test",
    "prettier": "prettier -c --write \"src/**/*\"",
    "start:dev": "cross-env REACT_APP_ENV=dev umi dev",
    "start:no-mock": "cross-env MOCK=none umi dev",
    "lint:es": "walrus eslint",
    "lint:style": "walrus stylelint --fix 'src/**/*.less' --syntax less",
    "lint:commit": "walrus commitlint --env HUSKY_GIT_PARAMS"
  },
  "dependencies": {
    "@alitajs/antd-plus": "^2.4.1",
    "@ant-design/charts": "^0.9.13",
    "@ant-design/icons": "^4.2.2",
    "@ant-design/pro-card": "^1.2.1",
    "@ant-design/pro-layout": "^6.4.19",
    "@formily/antd": "^1.3.3",
    "@formily/antd-components": "^1.3.3",
    "@pansy/hooks": "^2.2.0",
    "@pansy/react-aliplayer": "^0.3.2",
    "@pansy/react-charts": "^0.3.0",
    "antd": "^4.6.5",
    "classnames": "^2.2.6",
    "js-cookie": "^2.2.1",
    "moment": "^2.29.0",
    "numeral": "^2.0.6",
    "omit.js": "^2.0.2",
    "path-to-regexp": "^6.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet-async": "^1.0.7",
    "umi": "^3.2.22",
    "umi-request": "^1.3.5",
    "use-merge-value": "^1.0.2"
  },
  "devDependencies": {
    "@alitajs/umi-plugin-antd-plus": "^0.2.0",
    "@alitajs/umi-plugin-console-version": "^0.4.0",
    "@alitajs/umi-plugin-locale-plus": "^0.11.0",
    "@alitajs/umi-preset-react": "^0.2.3",
    "@types/classnames": "^2.2.10",
    "@types/js-cookie": "^2.2.6",
    "@types/numeral": "^0.0.28",
    "@types/react": "^16.9.49",
    "@types/react-dom": "^16.9.8",
    "@walrus/cli": "^1.3.4",
    "@walrus/plugin-release": "^1.9.1",
    "@walrus/preset-lint": "^1.1.7",
    "cross-env": "^7.0.2",
    "husky": "^4.3.0",
    "mockjs": "^1.1.0",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn prettier",
      "commit-msg": "yarn lint:commit"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie < 10"
  ],
  "engines": {
    "node": ">=10.13.0",
    "yarn": ">=1.3.2"
  }
}
