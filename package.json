{
  "name": "react-admin-template",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "bootstrap": "yarn || npm i",
    "clean": "rimraf dist",
    "start": "cross-env PORT=8888 umi dev",
    "build": "umi build",
    "analyze": "cross-env ANALYZE=1 umi build",
    "functions:build": "netlify-lambda build ./lambda",
    "functions:run": "cross-env NODE_ENV=dev netlify-lambda serve ./lambda",
    "site": "npm run build && npm run functions:build",
    "update-config": "walrus update-config",
    "lint": "walrus lint",
    "lint:style": "stylelint --fix 'src/**/*.less' --syntax less",
    "prettier": "walrus prettier",
    "test": "walrus test"
  },
  "repository": "https://github.com/ts-react/react-admin-template.git",
  "author": "JiuMao FE Team",
  "license": "MIT",
  "dependencies": {
    "@alitajs/antd-plus": "^1.0.10",
    "@alitajs/autils": "^0.5.2",
    "@alitajs/hooks": "^0.0.3",
    "@jiumao/rc-table": "^0.0.2",
    "antd": "^3.24.0",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "dva": "^2.6.0-beta.14",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.10",
    "memoize-one": "^5.0.0",
    "moment": "^2.22.2",
    "nprogress": "^0.2.0",
    "numeral": "^2.0.6",
    "path-to-regexp": "^3.0.0",
    "qs": "^6.7.0",
    "rc-charts": "^0.2.7",
    "react": "^16.8.6",
    "react-container-query": "^0.11.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.8.6",
    "react-media-hook2": "^1.0.5",
    "rollbar": "^2.4.4",
    "screenfull": "^4.2.0",
    "store": "^2.0.12"
  },
  "devDependencies": {
    "@alitajs/umi-plugin-deploy-config": "^0.1.2",
    "@alitajs/umi-plugin-lodash": "^0.1.0",
    "@babel/runtime": "^7.4.2",
    "@commitlint/cli": "^7.1.1",
    "@jiumao/commitlint-config": "^0.1.3",
    "@types/classnames": "^2.2.6",
    "@types/js-cookie": "^2.2.0",
    "@types/lodash": "^4.14.116",
    "@types/memoize-one": "^3.1.1",
    "@types/node": "^10.5.7",
    "@types/nprogress": "^0.0.29",
    "@types/qs": "^6.5.1",
    "@types/react": "^16.8.23",
    "@types/react-document-title": "^2.0.2",
    "@types/react-dom": "^16.8.4",
    "@types/store": "^2.0.0",
    "@walrus/cli": "^0.1.0",
    "@walrus/walrus-plugin-update-config": "^0.1.1",
    "babel-plugin-import": "^1.12.1",
    "body-parser": "^1.19.0",
    "cross-env": "^5.2.0",
    "eslint": "^5.14.1",
    "express": "^4.17.1",
    "husky": "^2.2.0",
    "identity-obj-proxy": "^3.0.0",
    "lint-staged": "^8.1.7",
    "lotus-lint": "^0.0.1",
    "mockjs": "^1.0.1-beta3",
    "netlify-lambda": "^1.5.1",
    "rimraf": "^2.6.2",
    "serverless-http": "^2.0.2",
    "stylelint": "^10.0.1",
    "stylelint-config-prettier": "^5.2.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^18.3.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "stylelint-order": "^3.0.0",
    "stylelint-prettier": "^1.1.1",
    "typescript": "^3.6.4",
    "umi": "^2.10.5",
    "umi-plugin-react": "^1.12.6",
    "umi-types": "^0.5.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn prettier",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less"
  },
  "engines": {
    "node": ">=8.9.0"
  }
}
